<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/fname"
        android:layout_width="103dp"
        android:layout_height="33dp"
        android:layout_marginBottom="28dp"
        android:layout_marginStart="16dp"
        android:text="First name"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/lname"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lname"
        android:layout_width="105dp"
        android:layout_height="30dp"
        android:layout_marginBottom="40dp"
        android:layout_marginStart="16dp"
        android:text="Last name"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/phonetxt"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/phonetxt"
        android:layout_width="105dp"
        android:layout_height="34dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:text="       Phone "
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/emailtxt"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/emailtxt"
        android:layout_width="98dp"
        android:layout_height="38dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:text="     E-mail"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/desigtxt"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/desigtxt"
        android:layout_width="100dp"
        android:layout_height="34dp"
        android:layout_marginBottom="148dp"
        android:layout_marginStart="16dp"
        android:text="Designation"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/submitbtn"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/submitbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="44dp"
        android:text="Submit"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/cprohome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="44dp"
        android:text="Home"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="179dp"
        android:layout_height="37dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:text="Create profile"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/fname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/fnamedt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/lnameedt"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/lnameedt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/phoneedt"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/phoneedt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/emailedt"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/emailedt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/desigedt"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/desigedt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="144dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/cprohome"
        app:layout_constraintEnd_toEndOf="parent" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <android.support.constraint.Barrier
        android:id="@+id/barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        tools:layout_editor_absoluteX="384dp" />

    <android.support.constraint.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</android.support.constraint.ConstraintLayout>